#include <bits/stdc++.h>
using namespace std;

int main() 
{
    int n;
    cin>>n;
    vector<int>v;
    for(int i=0;i<n;i++)
    {
        int x;
        cin>>x;
        v.push_back(x);
    }
    double d=0;
    for(int i=0;i<n;i++)
    {
        d+=double(v[i])/100;
    }
    cout<<(d/n)*100;
    
	return 0;
}
