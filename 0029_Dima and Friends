#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int n;
	cin>>n;
	int sum=0;
	int ans=0;
	int n1=n;
	while(n--)
	{
	    int x;
	    cin>>x;
	    sum+=x;
	}
	for(int i=1;i<=5;i++)
	{
	    if((sum+i)%(n1+1)!=1)
	    {
	        ans+=1;
	    }
	}
	cout<<ans;
	
	return 0;
}
