#include <bits/stdc++.h>
using namespace std;

int main() {
	// your code goes here
	string s;
	cin>>s;
	for(int i=0;i<s.size();i++)
	{
	    for(int j=i+1;j<s.size();j++)
	    {
	        if(s[i]>s[j]&&s[i]!='+'&&s[j]!='+')
	        {
	            swap(s[i],s[j]);
	        }
	    }
	}
	cout<<s;
	return 0;
}
