#include <bits/stdc++.h>
using namespace std;

int main() {
	int x,y,z;
	int n;
	int x1=0,y1=0,z1=0;
	cin>>n;
	while(n--)
	{
	    cin>>x;
	    x1+=x;
	    cin>>y;
	    y1+=y;
	    cin>>z;
	    z1+=z;
	}
// 	cout<<x1<<" "<<y1<<" "<<z1;
	if(x1==0&&y1==0&&z1==0)
	{
	    cout<<"YES"<<endl;
	}
	else
	{
	    cout<<"NO"<<endl;
	}
	return 0;
}
